<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.lotte.dengronnecafe.Start"
    android:background="@drawable/backgroundcafe">



    <TextView
        android:id="@+id/startvelkommen1"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="@string/start_text1"
        android:textColor="#060606"
        android:textSize="50dp"
        android:typeface="normal"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:id="@+id/startvelkommen2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/start_text2"
        android:textColor="#1e9306"
        android:textSize="50sp"
        android:textStyle="bold"
        android:layout_below="@+id/startvelkommen1"
        android:layout_alignRight="@+id/startvelkommen1"
        android:layout_alignEnd="@+id/startvelkommen1" />
    <TextView
        android:id="@+id/startvelkommen3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/start_text3"
        android:textColor="#060606"
        android:textSize="50sp"
        android:typeface="normal"
        android:layout_below="@+id/startvelkommen2"
         />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:textSize="50sp"
        android:textAllCaps="false"
        android:layout_above="@+id/botton_alignment1"
        android:layout_centerHorizontal="true">

        <Button
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:text="@string/start_menu"
            android:onClick="click_menu"
            android:background="#7969d95f"/>

        <Button
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:text="@string/start_profile"
            android:layout_marginBottom="30dp"
            android:onClick="click_profilemember"
            android:background="#7969d95f"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/botton_alignment1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true">

        <Button
            android:id="@+id/start_about"
            style="@style/Startbutton"
            android:text="@string/start_about"
/>
        <Button
            android:id="@+id/start_map"
            style="@style/Startbutton"
            android:text="@string/start_map"
            />
        <Button
            android:id="@+id/start_contact"
            style="@style/Startbutton"
            android:text="@string/start_contact"
            />
        <Button
            android:id="@+id/start_test"
            style="@style/Startbutton"
            android:text="@string/start_test"
            />

    </LinearLayout>
